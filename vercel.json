{
  "buildCommand": "bun run build",
  "installCommand": "bun install",
  "framework": "nextjs",
  "functions": {
    "app/**/*.ts": {
      "includeFiles": "**"
    }
  },
  "redirects": [
    {
      "source": "https://www.nicolasmenard.design/:path*",
      "destination": "https://nicolasmenard.design/:path*",
      "permanent": true
    }
  ]
}